# set slot 0 of metadata tag "CustomItemToken" of player to emerald with name "&aEmerald"
# open (metadata tag "GemBounderShop" of player) to player

function GemBounderShop(p: player):
	set {_clickToOpen} to mini message from "<gradient:#FFE259:#FFA751>[!] Click to select"
	set metadata tag "GemBounderShop" of {_p} to chest inventory with 5 rows named "&5Gem Bounder"
	set slot (integers between 0 and 44) of metadata tag "GemBounderShop" of {_p} to black stained glass pane named "&a"
	set slot 20 of metadata tag "GemBounderShop" of {_p} to orange dye of soul speed 1 named "&aTalismans" with hide enchants item flag with lore {_clickToOpen}
	set slot 22 of metadata tag "GemBounderShop" of {_p} to iron sword of soul speed 1 named "&cWeapons" with hide enchants item flag with lore {_clickToOpen}
	set slot 24 of metadata tag "GemBounderShop" of {_p} to black dye of soul speed 1 named "&bMiscellaneous" with hide enchants item flag with lore {_clickToOpen}
	open (metadata tag "GemBounderShop" of {_p}) to {_p}
	
