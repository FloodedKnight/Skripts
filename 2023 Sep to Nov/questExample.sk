function getCost(p: player, i: itemtype, n: integer) :: boolean:
	if {_p} has {_n} of {_i}:
		remove {_n} of {_i} from {_p}'s inventory
		return true
	else:
		return false

function getCompressedCost(p: player, i: itemtype, n: integer) :: boolean:
	set {_it} to {_i} with hide enchants item flag
	enchant {_it} with soul speed 
	if {_p} has {_n} of {_it}:
		remove {_n} of {_it} from {_p}'s inventory
		return true
	else:
		return false

