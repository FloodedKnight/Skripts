function IndigoDialogue(t: text, p: player):
	set {_b} to colored "§x§5§4§5§A§F§8%smallFont({_t})%"
	play sound "entity.parrot.ambient" at pitch 0 to {_p}
	message "" to {_p}
	message " &7[§x§6§4§0§0§F§F§lɪ§x§5§6§0§7§F§B§lɴ§x§4§8§0§F§F§7§lᴅ§x§3§A§1§6§F§4§lɪ§x§2§C§1§E§F§0§lɢ§x§1§E§2§5§E§C§lᴏ&7] §x§5§4§5§A§F§8%{_b}%" to {_p}
	message "" to {_p}

function dyeshop(p: player):
	open chest inventory with 3 rows named smallFont("&0dyes") to {_p}
	wait 1 tick
	set slot 0 of {_p}'s current inventory to black dye named "§x§2§0§2§0§2§0Black Dye" with lore "&7Cost: 64 Compressed Coal"
	set slot 3 of {_p}'s current inventory to white dye named "&fWhite Dye" with lore "&7Cost: 64 Compressed Iron"
	set slot 2 of {_p}'s current inventory to light gray dye named "§x§b§e§b§9§b§9Light Gray Dye" with lore "&7Cost: 4 Command Blocks"
	set slot 1 of {_p}'s current inventory to gray dye named "§x§6§2§6§0§6§0Gray Dye" with lore "&7Cost: 64 Compressed Stone"
	set slot 4 of {_p}'s current inventory to brown dye named "§x§6§2§3§f§0§aBrown Dye" with lore "&7Cost: 64 Compressed Wood"
	set slot 5 of {_p}'s current inventory to orange dye named "&6Orange Dye" with lore "&7Cost: 64 Golden Apples"
